@article{bresciani_pitfalls_2015,
	title = {The {Pitfalls} of {Visual} {Representations}: {A} {Review} and {Classification} of {Common} {Errors} {Made} {While} {Designing} and {Interpreting} {Visualizations}},
	volume = {5},
	issn = {2158-2440, 2158-2440},
