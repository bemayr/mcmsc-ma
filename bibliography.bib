@article{bean_prejudice_2012,
	title = {Prejudice {Concerns} and {Race}-{Based} {Attentional} {Bias}: {New} {Evidence} {From} {Eyetracking}},
	volume = {3},
	issn = {1948-5506, 1948-5514},
